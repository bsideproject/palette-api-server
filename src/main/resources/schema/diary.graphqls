type Diary {
    title: String!
    invitationCode: String!
    color: String
    users: [User]
    isDeleted:Boolean
}

extend type Mutation {
    createDiary(createDiaryInput: CreateDiaryInput): CreateDiaryOutput
}

type CreateDiaryOutput{
    title: String
    invitationCode: String
    color: String
}

input CreateDiaryInput {
    title: String!
    color: String!
}
